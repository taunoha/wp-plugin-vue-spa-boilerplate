{
  "name": "{plugin}",
  "version": "1.0.0",
  "scripts": {
    "dev": "vite",
    "build": "eslint ./src --fix && vite build",
    "lint": "npx stylelint ./src/assets/scss/**/*.scss --fix && npx stylelint ./src/{components,pages}/**/*.vue --fix && eslint ./src --fix",
    "rename": "node rename.js && npm run build"
  },
  "type": "module",
  "dependencies": {
    "@unhead/vue": "^1.11.20",
    "@vueuse/core": "^11.3.0",
    "@wordpress/i18n": "^5.21.0",
    "@wordpress/interactivity": "^6.21.0",
    "vue": "^3.5.13",
    "vue-router": "^4.5.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^8.29.0",
    "@vitejs/plugin-vue": "^5.2.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "eslint": "^9.23.0",
    "eslint-plugin-vue": "^9.33.0",
    "gettext-extractor": "^3.8.0",
    "prettier": "^3.5.3",
    "prompts": "^2.4.2",
    "sass": "^1.86.1",
    "stylelint": "^16.17.0",
    "stylelint-config-recess-order": "^5.1.1",
    "stylelint-config-recommended-vue": "^1.6.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.3",
    "unplugin-auto-import": "^0.18.6",
    "unplugin-vue-components": "^0.27.5",
    "vite": "^5.4.16"
  }
}
